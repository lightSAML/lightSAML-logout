{
    "name": "lightsaml/lightsaml-logout",
    "license": "GPL-3.0+",
    "type": "library",
    "description": "Light SAML2 logout php library",
    "autoload": {
        "psr-0": {
            "LightSaml\\Logout\\Tests\\": "tests/",
            "LightSaml\\Logout\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.1",
        "lightsaml/lightsaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "monolog/monolog": "~1.3",
        "satooshi/php-coveralls": "~0.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
